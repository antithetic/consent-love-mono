#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/raul/Documents/01 Projects/consent-love-mono/packages/sharedSchema/node_modules/loose-envify/node_modules:/Users/raul/Documents/01 Projects/consent-love-mono/packages/sharedSchema/node_modules:/Users/raul/Documents/01 Projects/consent-love-mono/packages/node_modules:/Users/raul/Documents/01 Projects/consent-love-mono/node_modules:/Users/raul/Documents/01 Projects/node_modules:/Users/raul/Documents/node_modules:/Users/raul/node_modules:/Users/node_modules:/node_modules:/Users/raul/Documents/01 Projects/consent-love-mono/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/raul/Documents/01 Projects/consent-love-mono/packages/sharedSchema/node_modules/loose-envify/node_modules:/Users/raul/Documents/01 Projects/consent-love-mono/packages/sharedSchema/node_modules:/Users/raul/Documents/01 Projects/consent-love-mono/packages/node_modules:/Users/raul/Documents/01 Projects/consent-love-mono/node_modules:/Users/raul/Documents/01 Projects/node_modules:/Users/raul/Documents/node_modules:/Users/raul/node_modules:/Users/node_modules:/node_modules:/Users/raul/Documents/01 Projects/consent-love-mono/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../loose-envify/cli.js" "$@"
else
  exec node  "$basedir/../loose-envify/cli.js" "$@"
fi
