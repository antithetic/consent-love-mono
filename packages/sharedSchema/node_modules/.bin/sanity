#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/raul/Documents/01 Projects/consent-love-mono/node_modules/.pnpm/sanity@3.91.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.0_@types+react@18.3.23_imm_1f99ee5cfef62dd45a7e688d3ded75db/node_modules/sanity/bin/node_modules:/Users/raul/Documents/01 Projects/consent-love-mono/node_modules/.pnpm/sanity@3.91.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.0_@types+react@18.3.23_imm_1f99ee5cfef62dd45a7e688d3ded75db/node_modules/sanity/node_modules:/Users/raul/Documents/01 Projects/consent-love-mono/node_modules/.pnpm/sanity@3.91.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.0_@types+react@18.3.23_imm_1f99ee5cfef62dd45a7e688d3ded75db/node_modules:/Users/raul/Documents/01 Projects/consent-love-mono/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/raul/Documents/01 Projects/consent-love-mono/node_modules/.pnpm/sanity@3.91.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.0_@types+react@18.3.23_imm_1f99ee5cfef62dd45a7e688d3ded75db/node_modules/sanity/bin/node_modules:/Users/raul/Documents/01 Projects/consent-love-mono/node_modules/.pnpm/sanity@3.91.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.0_@types+react@18.3.23_imm_1f99ee5cfef62dd45a7e688d3ded75db/node_modules/sanity/node_modules:/Users/raul/Documents/01 Projects/consent-love-mono/node_modules/.pnpm/sanity@3.91.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.0_@types+react@18.3.23_imm_1f99ee5cfef62dd45a7e688d3ded75db/node_modules:/Users/raul/Documents/01 Projects/consent-love-mono/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sanity/bin/sanity" "$@"
else
  exec node  "$basedir/../sanity/bin/sanity" "$@"
fi
